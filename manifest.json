{
    "manifest_version": 2,
    "name": "Youtube Playback Speed",
    "author": "Zineddine SAIBI",
    "version": "0.2",
    "description": "speed up or slow down the play rate of Youtube videos.",

    "icons": {
        "32": "icon/32.png",
        "64": "icon/64.png",
        "96": "icon/96.png",
        "128": "icon/128.png",
        "256": "icon/256.png",
        "512": "icon/512.png"
    },

    "content_scripts": [
        {
            "matches":
            [
                "<all_urls>"
            ],
            "js": ["src/js/content.js"]
        }
    ],

    "browser_action": {
        "default_icon": "icon/icon.png",
        "default_popup": "src/index.html"
    },

    "permissions": [
        "tabs",
        "<all_urls>",
        "activeTab",
        "storage"
    ]
}
